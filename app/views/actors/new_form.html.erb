<h1>Add A New Actor</h1>

<form action="http://localhost:3000/create_actor">
  <div>
    <label for="name">Name:</label>
    <input id="name" type="string" name="name">
  </div>
  <div>
    <label for="bio">Bio:</label>
    <input id="bio" type="text" name="bio">
  </div>
  <div>
    <label for="dob">DOB:</label>
    <input id="dob" type="string" name="dob">
  </div>
  <div>
    <label for="image_url">Image URL:</label>
    <input id="image_url" type="string" name="image_url">
  </div>
  <div>
    <button>Create Actor</button>
  </div>
</form>
