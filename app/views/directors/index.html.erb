<h1>All Directors</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Bio</th>
    <th>DOB</th>
    <th>Image</th>
  </tr>
<% @list_of_directors.each do |director| %>
  <tr>
    <td><%= director.name %></td>
    <td><%= director.bio%></td>
    <td><%= director.dob %></td>
    <td><img src ="<%= director.image_url %>"></td>
  </tr>
  <% end %>
</table>
